{
  "extends": ["prettier/plugin-php"],
  "plugins": ["html", "php-markup"],
  "settings": {
    "php/php-extensions": [".php"],
    "php/markup-replacement": { "php": "", "=": "0" },
    "php/keep-eol": false,
    "php/remove-whitespace": false,
    "php/remove-empty-line": false,
    "php/remove-php-lint": false
  }
}
